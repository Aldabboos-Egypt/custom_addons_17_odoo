<!-- res.partner -->
<record id="demo_res_partner_fields" model="fetch.data">
    <field name="model_id" ref="base.model_res_partner"/>
    <field name="field_ids" model="ir.model.fields" search="[('name', 'in',
        ['category_id', 'city', 'comment', 'id', 'image_128', 'mobile', 'name', 'phone',
        'property_product_pricelist', 'sales_persons_ids', 'state_id', 'street', 'user_id', 'user_ids']),
        ('model_id.model','=','res.partner')
        ]"/>
</record>

<!-- product.category -->
<record id="demo_product_category_fields" model="fetch.data">
    <field name="model_id" ref="product.model_product_category"/>
    <field name="field_ids" model="ir.model.fields" search="[('name', 'in',
        ['child_id', 'id', 'name', 'parent_id']),
        ('model_id.model','=','product.category')
        ]"/>
</record>

